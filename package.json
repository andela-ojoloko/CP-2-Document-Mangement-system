{
  "name": "dms",
  "version": "1.0.0",
  "description": "a document management system",
  "main": "index.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "start":"babel-node serverProd.js",
    "seed": "sequelize db:seed --seed roles.js && sequelize db:seed --seed users.js && sequelize db:seed --seed documents.js  ",
    "start:dev": "nodemon --exec babel-node serverDev.js",
    "test": "NODE_ENV=test nyc --reporter=text  --reporter=html --extension .jsx ./node_modules/mocha/bin/_mocha client/tests/testSetup.js client/tests/components/**/*spec.js client/tests/**/*spec.js server/tests/**/*.spec.js --timeout=60000",
    "test:server": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/mocha/bin/_mocha --compilers js:babel-register server/tests/**/*.spec.js --timeout=60000",
    "test:client": "NODE_ENV=test nyc --reporter=text  --reporter=html --extension .jsx ./node_modules/mocha/bin/_mocha client/tests/testSetup.js client/tests/components/**/*spec.js client/tests/**/*spec.js --timeout=60000",
    "clean-dist": "npm run remove-dist && mkdir -p client/dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "remove-dist": "node_modules/.bin/rimraf client/dist",
    "build:html": "./node_modules/.bin/babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "./node_modules/.bin/babel-node tools/build.js",
    "postbuild": "./node_modules/.bin/babel-node serverProd.js"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "server/tests",
      "client/tests"
    ]
  },
  "author": "Oluwagbenga Joloko",
  "license": "ISC",
  "dependencies": {
